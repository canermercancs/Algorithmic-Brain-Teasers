     ]
    }
   ],
   "source": [
    "from dynamic_programming.partition import equalSums\n",
    "\n",
    "arr = [3,5,6,8]\n",
    "print(equalSums(arr))\n",
    "\n",
    "arr = [3,5,6,8]*3\n",
    "print(equalSums(arr))\n",
    "\n",
    "arr = [3,5,6,8]*3 + [1]\n",
    "print(equalSums(arr))"
   ]
